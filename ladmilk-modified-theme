    {block:ifRecentUpdates}
    <script src="https://static.tumblr.com/3yblkz0/VwZnm51yb/slick.min.js"></script>
    {/block:ifRecentUpdates}
    <script src="https://static.tumblr.com/qudkd6d/Az6nkemqr/pxuphotoset.min.js"></script>
    <script src="https://static.tumblr.com/3yblkz0/Bu3nywvqg/shythemes_vr.js"></script>
     
    {block:IndexPage}
    <script src="https://static.tumblr.com/3yblkz0/qEunz27je/imagesloaded.pkgd.min.js"></script>
    {block:ifMasonry}
    <script src="https://static.tumblr.com/wgijwsy/Ebfm2v4gy/jquery.masonry.min.js"></script>
    {/block:ifMasonry}
    {block:ifInfinitescroll}
    <script src="https://static.tumblr.com/wgijwsy/u2vm2hxv6/jquery.infinitescroll.min.js"></script>
    {/block:ifInfinitescroll}
    {/block:IndexPage}
    
    
    <script>
    $(document).ready(function(){
    {block:ifNews}
    $('.spot').click(function(){
        $(".spotlight_outer").fadeIn();
        });
    $('.ov').click(function(){
        $(".spotlight_outer").fadeOut();
        });
    {/block:ifNews}
    {block:ifRecentUpdates}
    $('.updates').slick({
        autoplay: true,
        autoplaySpeed: 3000,
        arrows: false,
        slidesToShow: 1,
        infinite: true,
        dots:true,
        variableWidth:true,
        });
    {/block:ifRecentUpdates}
    {block:ifStickyNavigation}
    var sticky = $('nav').offset().top;
    $(window).scroll(function(){
        if( $(window).scrollTop() > sticky ) {
            $('nav').addClass('nn');
            } else {
            $('nav').removeClass('nn');
            }
        });
    {block:ifAbout}{block:ifIconImage}
    var icon = $('.ic').offset().top;
    $(window).scroll(function(){
        if( $(window).scrollTop() >= icon) {
            $('.title').addClass('cc');
            } else {
            $('.title').removeClass('cc');
            }
        });
    {/block:ifIconImage}{/block:ifAbout}
    {/block:ifStickyNavigation}
    {block:IndexPage}
    {block:ifnotInfinitescroll}
    {block:ifStickyPagination}
    $(window).scroll(function(){
        var pa = $('.pa').outerHeight();
        if($(document).height() - ($(window).height() + $(this).scrollTop()) > ($( "footer" ).height() + pa)) {
            $('.pagination').addClass('pp');
            } else {
            $('.pagination').removeClass('pp');
            }
        if( $(window).scrollTop() ) {
            $('.pagination').css({opacity: '1'});
            } else {
            $('.pagination').css({opacity: '0'});
            }
        });
    {/block:ifStickyPagination}
    {/block:ifnotInfinitescroll}
    {/block:IndexPage}
    $('.photo-slideshow').pxuPhotoset({
        lightbox: true,
        rounded: false,
        gutter: '5px',
        borderRadius: '0px',
        photoset: '.photo-slideshow',
        photoWrap: '.photo-data',
        photo: '.pxu-photo'
        });
    {block:IndexPage}
    var $container = $('#wrapper');
    {block:ifMasonry}
    $container.masonry({
        itemSelector: 'article',
        columnWidth: {select:Post Size} + {select:Margin}*2,
        isFitWidth: true,
        });
    $container.imagesLoaded(function(){
        $container.masonry();
        });
    {/block:ifMasonry}
    {block:ifInfinitescroll}
    $container.infinitescroll({
        itemSelector : "article",
        navSelector : ".pagination",
        nextSelector : ".pagination a#next",
        loadingImg : "",
        loadingText : "<em> </em>",
        bufferPx : 1000,
        extraScrollPx: 500,
        {block:ifManualLoad}
        errorCallback: function () { jQuery('a#load').delay(50).fadeOut(); },
        {/block:ifManualLoad}
        },
    function( newElements ) {
        resizeVideos();
        $(newElements).find('.photo-slideshow').pxuPhotoset({
            lightbox: true,
            rounded: false,
            gutter: '5px',
            borderRadius: '0px',
            photoset: '.photo-slideshow',
            photoWrap: '.photo-data',
            photo: '.pxu-photo'
        });
    {block:ifMasonry}
        var $newElems = $(newElements).hide();
        $newElems.imagesLoaded(function(){
            $newElems.fadeIn();
            $container.masonry( 'appended', $newElems, true );
        });
    {/block:ifMasonry}
    {block:ifDraggableTags}
        $(newElements).find(".post_tags_inner").draggable({
            axis: "x",
            scroll: false,
            stop: function() {
            var pt_left = $(this).css("left").replace(/[^-\d\.]/g, '');
            if(pt_left > 0){ $(this).animate({left: 0}, 400, 'easeOutExpo');}
            var pt_width = $(this).outerWidth();
            var pt_parentWidth = $(".post_tags.draggable").outerWidth();
            if(pt_width > pt_parentWidth) {
            if(pt_left < pt_parentWidth - pt_width ){
            $(this).animate({left: pt_parentWidth - pt_width}, 400, 'easeOutExpo'         );}
            } else {
            $(this).animate({left: 0}, 400, 'easeOutExpo');}
            }
        });
    {/block:ifDraggableTags}
    {block:ifLikeButton}
    {block:ifnotMasonry}var $newElems = $(newElements);{/block:ifnotMasonry}
        var $newElemsIDs = $newElems.map(function (){return this.id;}).get();
            Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
    {/block:ifLikeButton}
        });
    {block:ifManualLoad}
    $(window).unbind('.infscr');
    $('a#load').click(function() {
        $container.infinitescroll('retrieve');return false;
        });
    {/block:ifManualLoad}
    {/block:ifInfinitescroll}
    {/block:IndexPage}
    });  
    </script>
    
    
    {block:ifDraggableTags}
    <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>
    
    <script>
    $(document).ready(function(){
    $(".post_tags_inner").draggable({
      axis : "x",
      scroll: false,
      stop: function() {
        var pt_left = $(this).css("left").replace(/[^-\d\.]/g, '');
        if(pt_left > 0){
        $(this).animate({left: 0}, 400, 'easeOutExpo');
        }
        var pt_width = $(this).outerWidth();
        var pt_parentWidth = $(".post_tags.draggable").outerWidth();
        if(pt_width > pt_parentWidth) {
        if(pt_left < pt_parentWidth - pt_width){
        $(this).animate({left: pt_parentWidth - pt_width}, 400, 'easeOutExpo');
        }
        } else {
        $(this).animate({left: 0}, 400, 'easeOutExpo');
        }
        }
        });
    });
    </script>
    {/block:ifDraggableTags}
     
     
     
